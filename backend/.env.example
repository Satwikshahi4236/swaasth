# Database Configuration
DATABASE_URL=postgresql://swaasth_user:swaasth_password@localhost:5432/swaasth_db
POSTGRES_USER=swaasth_user
POSTGRES_PASSWORD=swaasth_password
POSTGRES_DB=swaasth_db

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30

# Redis Configuration (for Celery)
REDIS_URL=redis://localhost:6379/0

# Firebase Configuration (for push notifications)
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_PRIVATE_KEY_ID=your-private-key-id
FIREBASE_PRIVATE_KEY=your-private-key
FIREBASE_CLIENT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your-client-id
FIREBASE_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40your-project.iam.gserviceaccount.com

# CORS Configuration
CORS_ORIGINS=["http://localhost:3000", "http://localhost:19006", "exp://192.168.1.100:19000"]

# Sentry Configuration (Error Tracking)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# App Configuration
APP_NAME="Swaasth Elder Health API"
APP_VERSION="1.0.0"
DEBUG=true